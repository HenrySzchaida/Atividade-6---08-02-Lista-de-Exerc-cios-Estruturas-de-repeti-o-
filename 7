programa {
	funcao inicio() {
		
	    inteiro nota
	    nota = 11
	    enquanto(nota <= -1 ou nota >= 11){
		escreva("\n Se tiver como Avalie nosso trabalho com uma nota de 0 a 10 \n")
		leia(nota)
	    }
	    se(nota >= 0 e nota <= 10){
		    escreva("\n Obrigado senhor(a) conte sempre com a gente! \n")
	    }
	}
}
