programa {
	funcao inicio() {
		cadeia senha
		
		senha = ""
		escreva("\n Digite a senha de sua escolha. \n")
		leia(senha)
		
		enquanto(senha == "12345" ou senha == "xuxa" ou senha == "admin" ou senha == "senha"){
		escreva("\n Senha incorreta! Tente novamente. \n")
		leia(senha)
		}
		escreva("Senha correta com sucesso!")
	}
}
