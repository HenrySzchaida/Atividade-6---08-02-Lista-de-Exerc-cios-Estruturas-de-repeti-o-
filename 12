programa {
		inclua biblioteca Matematica --> mat
	
    	funcao inicio()
    	{
    		real num1, num2, num3, num4, num5, maior
    
    		escreva("Informe o 1 número: ")
    		leia(num1)
    
    		escreva("Informe o 2 número: ")
    		leia(num2)
    		
    		escreva("Informe o 3 número: ")
    		leia(num3)
    		
    		escreva("Informe o 4 número: ")
    		leia(num4)
    		
    		escreva("Informe o 5 número: ")
    		leia(num5)
    		
    		maior = mat.maior_numero(num1, num2, num3, num4, num5)
    		escreva("\nO maior número é: ", maior, "\n")
    }
}
